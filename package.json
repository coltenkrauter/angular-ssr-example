{
  "name": "angular-ssr-example",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr:prod": "npm run build:client-and-server-bundles:prod && npm run compile:server",
    "build:ssr:cert": "npm run build:client-and-server-bundles:cert && npm run compile:server",
    "build:ssr:dev": "npm run build:client-and-server-bundles:dev && npm run compile:server",
    "build:client-and-server-bundles:prod": "ng build --configuration=prod-ssr && ng run angular-ssr-example:server:prod-ssr --bundleDependencies all",
    "build:client-and-server-bundles:cert": "ng build --configuration=cert-ssr && ng run angular-ssr-example:server:cert-ssr --bundleDependencies all",
    "build:client-and-server-bundles:dev": "ng build --configuration=dev-ssr && ng run angular-ssr-example:server:dev-ssr --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-next.0",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "~9.0.0-next.0",
    "@angular/compiler": "~9.0.0-next.0",
    "@angular/core": "~9.0.0-next.0",
    "@angular/forms": "~9.0.0-next.0",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "~9.0.0-next.0",
    "@angular/platform-browser-dynamic": "~9.0.0-next.0",
    "@angular/platform-server": "~9.0.0-next.0",
    "@angular/router": "~9.0.0-next.0",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@ng-toolkit/universal": "^7.1.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "angular-gauge": "^3.1.2",
    "express": "^4.15.2",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.0",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~9.0.0-next.0",
    "@angular/language-service": "~9.0.0-next.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.0"
  }
}
